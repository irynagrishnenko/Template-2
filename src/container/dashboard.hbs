<html>

  <head>
    <title>
      Dashboard page
    </title>
    <meta charset='utf-8' />

    <link
      rel='apple-touch-icon'
      sizes='180x180'
      href='/fav/apple-touch-icon.png'
    />
    <link
      rel='icon'
      type='image/png'
      sizes='32x32'
      href='/fav/favicon-32x32.png'
    />
    <link
      rel='icon'
      type='image/png'
      sizes='16x16'
      href='/fav/favicon-16x16.png'
    />
    <link rel='manifest' href='/fav/site.webmanifest' />
    <link
      rel='mask-icon'
      href='/fav/safari-pinned-tab.svg'
      color='#5bbad5'
    />
    <meta
      name='msapplication-TileColor'
      content='#da532c'
    />
    <meta name='theme-color' content='#ffffff' />

    <link href='css/normalize.css' rel='stylesheet' />

    <link href='css/dashboard.css' rel='stylesheet' />

    <meta
      name='viewport'
      content='viewport-fit=cover, width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no'
    />

  </head>

  <body>

    <div class='page page--dashboard'>
      <aside
        class='aside page__aside vertical vertical--big'
      >
        <div class='text aside__logo'>Sidebar</div>
        <nav class='vertical vertical--small'>
          <a
            href='#'
            class='nav-button nav-button--active click'
          >
            <span
              class='icon nav-button__icon'
              style="background-image: url('/svg/nav/home.svg');"
            ></span>
            <span class='text text--light'>Home</span>
          </a>
          <a href='#' class='nav-button click'>
            <span
              class='icon nav-button__icon'
              style="background-image: url('/svg/nav/orders.svg');"
            ></span>
            <span class='text text--light'>Orders</span>
          </a>
          <a href='#' class='nav-button click'>
            <span
              class='icon nav-button__icon'
              style="background-image: url('/svg/nav/products.svg');"
            ></span>
            <span class='text text--light'>Products</span>
          </a>
        </nav>
      </aside>
      <header class='header'>
        <div class='layout header__content text--small'>
          <div class='header__search relative'>
            <input
              type='text'
              placeholder='Search'
              class='text field field--header'
            />
            <span
              class='icon header__icon-search'
              style="background-image: url('/svg/header/search.svg');"
            ></span>
            <span
              class='icon header__icon-settings icon--button icon--color-blue click'
              style="background-image: url('/svg/header/settings.svg');"
            ></span>
          </div>
          <div class='header__icons'>
            <span
              class='icon header__icon-general icon--button icon--color-yellow click'
              style="background-image: url('/svg/header/wifi.svg');"
            ></span>
            <span
              class='icon header__icon-general icon--button icon--color-blue2 click'
              style="background-image: url('/svg/header/symbols.svg');"
            ></span>
            <span
              class='icon header__icon-general icon--button icon--color-yellow click'
              style="background-image: url('/svg/header/bell.svg');"
            ></span>
          </div>
        </div>
      </header>
      <main class='main'>
        <div class='layout'>
          <section class='box analytic__block'>
            <div
              class='analytic analytic--double analytic--purple'
            >
              <span
                class='icon analytic__button icon--button icon--color-purple click'
                style="background-image: url('/svg/analytic/money.svg');"
              ></span>
              <div
                class='relative vertical vertical--small'
              >
                <div
                  class='analytic__title text text--light'
                >$500.00</div>
                <div class='text text--lighter'>Total
                  earning</div>
                <span
                  class='icon analytic__arrow'
                  style="background-image: url('/svg/analytic/blue-arrow.svg');"
                ></span>
              </div>
            </div>
            <div
              class='analytic analytic--double analytic--blue'
            >
              <span
                class='icon analytic__button icon--button icon--color-blue click'
                style="background-image: url('/svg/analytic/order.svg');"
              ></span>
              <div
                class='relative vertical vertical--small'
              >
                <div
                  class='analytic__title text text--light'
                >$961</div>
                <div class='text text--lighter'>Total Order</div>
                <span
                  class='icon analytic__arrow'
                  style="background-image: url('/svg/analytic/blue-arrow.svg');"
                ></span>
              </div>
            </div>

            <div class='analytic analytic--blue horizontal'>
              <span
                class='icon analytic__button icon--button icon--color-blue click'
                style="background-image: url('/svg/analytic/money.svg');"
              ></span>
              <div class='analytic__content'>
                <div
                  class='text text--medium text--light'
                >$203k</div>
                <div class='text text--lighter'>Total Income</div>
              </div>
            </div>
            <div
              class='analytic analytic--white horizontal analytic__border'
            >
              <span
                class='icon analytic__button icon--button icon--color-yellow click'
                style="background-image: url('/svg/analytic/shop.svg');"
              ></span>
              <div class='analytic__content'>
                <div class='text text--medium'>$203k</div>
                <div class='text text--gray'>Total Income</div>
              </div>
            </div>
          </section>
        </div>
      </main>
      <div class='main'>
        <div class='layout'>
          <div class='order'>
            <section class='purchase__block box'>
              <h2 class='text text--heading'>Purchase list</h2>
              <hr class='divider' />
              <div class='id__order'>
                <div class='order__block'>
                  <span class='text'>ID437661</span>
                  <span class='text'>May 11,2023,14:21</span>
                </div>
                <div class='order__block'>
                  <span class='text'>AirOn ProCam</span>
                  <span class='text'>7/8 USB Type-C</span>
                </div>
                <div class='order__block'>
                  <span class='text'>132.32 EUR</span>
                  <span class='text text--red'>Not paid</span>
                </div>
                <div class='order__block'>
                  <span class='text'>Customer</span>
                  <span class='text'>robin@com.de</span>
                </div>
              </div>
              <hr class='divider' />
              <div class='id__order'>
                <div class='order__block'>
                  <span class='text'>ID437661</span>
                  <span class='text'>May 11,2023,14:21</span>
                </div>
                <div class='order__block'>
                  <span class='text'>PHILIPS Essential</span>
                  <span class='text'>HD9200/90</span>
                </div>
                <div class='order__block'>
                  <span class='text'>543 USD</span>
                  <span class='text text--green'>Pay by
                    Stripe</span>
                </div>
                <div class='order__block'>
                  <span class='text'>Customer</span>
                  <span class='text'>dima@com.ua</span>
                </div>
              </div>
            </section>
            <section class='purchase__block box'>
              <h3 class='text text--heading'>Payment</h3>
              <hr class='divider' />
              <div class='payment__radio'>
                <input
                  type='radio'
                  name='payment'
                  id='payment'
                />
                <label
                  class='text text--heading-small'
                  for='payment'
                >Credit card</label>
              </div>
              <div class='payment__radio'>
                <input
                  type='radio'
                  name='payment'
                  id='payment'
                />
                <label
                  class='text text--heading-small'
                  for='payment'
                >Debit card</label>
              </div>
              <div class='payment__radio'>
                <input
                  type='radio'
                  name='payment'
                  id='payment'
                />
                <label
                  class='text text--heading-small'
                  for='payment'
                >PayPal</label>
              </div>
              <div class='info__card'>
                <div class='card__field'>
                  <span
                    class='text text--heading-small'
                  >Name on card</span>
                  <input type='text' class='field' />
                </div>
                <div class='card__field'>
                  <span
                    class='text text--heading-small'
                  >Credit card number</span>
                  <input type='text' class='field' />
                </div>
              </div>
              <div class='info__card'>
                <div class='card__field'>
                  <span
                    class='text text--heading-small'
                  >Expiration</span>
                  <input type='text' class='field' />
                </div>
                <div class='card__field'>
                  <span
                    class='text text--heading-small'
                  >CVV</span>
                  <input type='text' class='field' />
                </div>
              </div>
              <hr class='divider' />
              <div class='info__card'>
                <div class='relative card__field'>
                  <span
                    class='text text--heading-small'
                  >Country</span>
                  <div class='field field--select relative'>
                    <span
                      class='text text--heading-small'
                    >Choose...</span>
                    <span
                      class='icon icon__select'
                      style="background-image: url('/svg/select.svg');"
                    ></span>
                    <select class='field__select click'>
                      <option value='dog'>Dog</option>
                      <option value='cat'>Cat</option>
                      <option
                        value='hamster'
                      >Hamster</option>
                      <option value='parrot'>Parrot</option>
                      <option value='spider'>Spider</option>
                    </select>
                  </div>
                </div>
                <div class='relative card__field'>
                  <span
                    class='text text--heading-small'
                  >State</span>
                  <div class='field field--select relative'>
                    <span
                      class='text text--heading-small'
                    >Choose...</span>
                    <span
                      class='icon icon__select'
                      style="background-image: url('/svg/select.svg');"
                    ></span>
                    <select class='field__select click'>
                      <option value='dog'>Dog</option>
                      <option value='cat'>Cat</option>
                      <option
                        value='hamster'
                      >Hamster</option>
                      <option value='parrot'>Parrot</option>
                      <option value='spider'>Spider</option>
                    </select>
                  </div>
                </div>
              </div>
              <hr class='divider' />
              <a
                href='#'
                class='nav-button nav-button--active nav-button--center click'
              >
                <span class='text text--light'>Continue to
                  checkout</span>
              </a>
            </section>
          </div>
        </div>
      </div>
    </div>

  </body>

</html>